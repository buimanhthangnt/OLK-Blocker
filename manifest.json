{
	"name": "B.M.T Privacy",
	"version": "1.0",
	"description": "Facebook blocker",
	"permissions": [
		"webRequest",
		"storage",
		"webRequestBlocking",
		"http://*/",
		"https://*/"
	],
	"background": {
		"scripts": [
			"background.js"
		]
	},
	"browser_action": {
		"default_icon": "icon.png",
		"default_popup": "popup.html",
		"icons": {
			"16": "icon.png",
			"48": "icon.png",
			"128": "icon.png"
		},
		"default_title": "B.M.T Privacy"
	},
	"author": "Bùi Mạnh Thắng",
	"manifest_version": 2
}